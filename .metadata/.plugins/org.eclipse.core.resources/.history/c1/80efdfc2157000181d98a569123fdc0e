from pathlib import Path

def fileLines(path_to_file):
    # Return lines of a txt file in a list
    
    # Checking if file exists     
    if file_to_open.exists():
        print("Yay, the file exists!")
        f = open(path_to_file,"r")
        # Getting all lines    
        all_lines = f.readlines()
        f.close()
        print('Number of lines: ', str(len(all_lines)),'felix')
        return all_lines
    
    print("Oops, file doesn't exist!")
    return 0


data_folder = Path("C:/Users/155 X-MX/Desktop/Preenche Servicos")
file_to_open = data_folder / "dados.txt"

print(type(file_to_open))
    
fileLines(file_to_open)